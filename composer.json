{
    "name": "farmto/shippo-laravel",
    "description": "Laravel integration for Shippo Api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Mafrtin Cabucos",
            "email": "christianmartincabucos@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "*",
        "shippo/shippo-php": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "FarmTo\\ShippoCarriers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FarmTo\\ShippoCarriers\\ShippoCarriersServiceProvider"
            ],
            "aliases": {
                "ShippoCarrier": "FarmTo\\ShippoCarriers\\Facades\\ShippoCarrier"
            }
        }
    },
    "minimum-stability": "stable"
}
